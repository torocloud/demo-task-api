                            "to": [
                                "insertTaskInput",
                                "assignedUserId"
                            ]
                        },
                        {
                            "type": "set",
                            "expression": "true",
                            "to": [
                                "insertTaskInput",
                                "$getGeneratedKeys"
                            ]
                        },
                        {
                            "type": "set",
                            "expression": "final taskStatusFormat = [\n\t'Not Started' : 'Not Started',\n\t'In Progress' : 'In Progress',\n\t'Completed' : 'Completed',\n\t'Closed' : 'Closed'\n]\n\nreturn taskStatusFormat[task.status] ?: 'Not Started'",
                            "evaluate": true,
                            "to": [
                                "insertTaskInput",
                                "status"
                            ]
                        }
                    ],
                    "outputs": [
                        {
                            "from": [
                                "updateCount"
                            ],
                            "to": [
                                "updateCount"
                            ]
                        },
                        {
                            "type": "set",
                            "expression": "$gloopOutput.generatedKeys.taskId",
                            "evaluate": true,
                            "to": [
                                "task",
                                "taskId"
                            ]
