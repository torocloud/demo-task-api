                            "to": [
                                "insertTaskInput",
                                "subject"
                            ]
                        },
                        {
                            "from": [
                                "task",
                                "description"
                            ],
                            "to": [
                                "insertTaskInput",
                                "description"
                            ]
                        },
                        {
                            "from": [
                                "task",
                                "deadline"
                            ],
                            "to": [
                                "insertTaskInput",
                                "deadline"
