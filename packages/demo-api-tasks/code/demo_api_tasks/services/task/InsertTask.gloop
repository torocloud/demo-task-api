                                        },
                                        {
                                            "type": "set",
                                            "expression": "UUID.randomUUID()",
                                            "evaluate": true,
                                            "to": [
                                                "internalId"
                                            ]
                                        },
                                        {
                                            "type": "set",
                                            "expression": "Task",
                                            "to": [
                                                "documentTypeId"
                                            ]
                                        },
