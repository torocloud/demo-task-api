                },
                {
                    "type": "invokeGloop",
                    "name": "demo_api_tasks.services_sql.task.InsertTask",
                    "declare": [
                        {
                            "variables": [
                                {
                                    "name": "updateCount",
                                    "type": "integer"
                                }
                            ]
